#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules/next/dist/bin/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules/next/dist/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules/next/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules/next/dist/bin/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules/next/dist/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules/next/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/next@15.4.6_@babel+core@7.28.3_@opentelemetry+api@1.9.0_@playwright+test@1.55.0_react-d_369d37965ae8cf0a48106bfbb0e6af64/node_modules:/home/gi/Documents/1_Projects/afarsemon/afarsemon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
